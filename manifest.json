{
  "manifest_version": 2,
  "name": "start-page-trello",
  "short_name": "startpage",
  "version": "0.1",
  "description": "Overrides the browsers newtab page with a custom startpage.",
  "author": "alai",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "128":"img/icon.png"
  },
  "permissions": ["storage"],
  "content_security_policy": "script-src 'self' 'unsafe-inline' 'unsafe-hashes' https://kit.fontawesome.com/92cbdcb18b.js https://code.jquery.com/jquery-3.3.1.min.js https://api.trello.com/1/client.js?key=ad16b3881276bec6ac47d39a77c62342; object-src 'self'"

}
